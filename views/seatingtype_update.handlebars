<h1>Update Seating</h1>
<div>
  <form method="POST">
    <div>
      <label>Update Seating Type</label>
      <select name="name">
        {{#each typename}}
          <option value="{{this.id }}">{{this.name}}</option>
        {{/each}}
      </select>
    </div>

    <input type="hidden" value={{id}} name="id" />
    <input type="submit" />
  </form>
</div>
